{
  "version": 2,
  "builds": [
    {
      "src": "**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    { "handle": "filesystem" },
    { "src": "/(.*)", "dest": "/" }
  ],
  "rewrites": [
    { "source": "/api/:match*", "destination": "/api/:match*" },
    { "source": "/:path*", "destination": "/path*" }
  ]
}
